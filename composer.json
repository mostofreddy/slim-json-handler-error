    "keywords": ["slim", "framework", "micro", "error", "error handler", "json", "rest", "restfull", "resty", "restyphp"],
    "homepage": "https://github.com/mostofreddy/slim-json-handler-error",
    "license": "MIT",
    "version": "0.3.0",
    "authors": [
        {
            "name": "Federico Lozada Mosto",
            "email": "mosto.federico@gmail.com",
            "homepage": "http://www.mostofreddy.com.ar"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "restyphp/json-api-error": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.5.*",
        "squizlabs/php_codesniffer": "2.7.*",
        "slim/slim": "3.8.*"
    },
    "autoload": {
        "psr-4": {
            "Resty\\Slim\\": "src/",
            "Resty\\Slim\\Test\\": "tests/src/"
        }
    }
